   <children>
      <TextField fx:id="deckName" layoutX="229.0" layoutY="224.0" />
      <Label layoutX="229.0" layoutY="199.0" text="Type the name of the deck you want to create" textFill="#dd2222" />
      <Button layoutX="238.0" layoutY="258.0" onMouseClicked="#createDeck" mnemonicParsing="false" text="done" />
      <Button layoutX="238.0" layoutY="456.0" mnemonicParsing="false" onMouseClicked="#goBack" text="Back" />
   </children>
</AnchorPane>
